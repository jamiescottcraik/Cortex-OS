{
  "name": "@cortex-os/mcp-bridge",
  "version": "0.1.0",
  "description": "MCP plugin management and marketplace system",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/python/**"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./client/index.ts",
      "default": "./client/index.ts"
    },
    "./auth": {
      "types": "./auth/index.ts",
      "default": "./auth/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node -e \"require('fs').cpSync('src/python','dist/python',{recursive:true})\"",
    "test": "vitest",
    "demo": "tsx src/mcp-demo-server.ts",
    "demo:build": "tsc && node dist/mcp-demo-server.js",
    "mlx:embed": "tsx src/scripts/embed-mlx-neuron.mjs",
    "mlx:start": "tsx src/scripts/embed-mlx-neuron.mjs",
    "mlx:config": "node src/scripts/setup-mlx-config.mjs --profile recommended --out ./mlx.json",
    "start:complete": "npm run mlx:embed",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "commander": "^12.1.0",
    "express": "^4.18.2",
    "zod": "^3.22.0"
  }
}

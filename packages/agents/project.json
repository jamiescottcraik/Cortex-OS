{
  "name": "@cortex-os/agents",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/agents/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "packages/agents"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm test",
        "cwd": "packages/agents"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm typecheck",
        "cwd": "packages/agents"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "packages/agents/src/**/*.ts",
          "packages/agents/tests/**/*.ts"
        ]
      }
    }
  }
}
{
  "name": "@cortex-os/agents",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm",
    "dev": "tsup src/index.ts --dts --format esm --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@cortex-os/a2a": "workspace:*",
    "@cortex-os/mcp-bridge": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}

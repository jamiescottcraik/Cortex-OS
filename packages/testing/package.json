{
  "name": "@cortex-os/memory-testing",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:parity": "vitest run --config vitest.parity.config.ts"
  },
  "dependencies": {
    "@cortex-os/memory-core": "workspace:*",
    "@cortex-os/mcp-server": "workspace:*",
    "@cortex-os/memory-rest-api": "workspace:*",
    "@cortex-os/tool-spec": "workspace:*",
    "supertest": "^6.3.4",
    "@modelcontextprotocol/sdk": "^0.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
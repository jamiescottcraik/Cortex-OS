{
  "name": "@cortex-os/kernel-backup",
  "private": true,
  "version": "1.0.0",
  "description": "Deterministic kernel for Cortex-OS with LangGraph state management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./graph": {
      "types": "./dist/graph-simple.d.ts",
      "import": "./dist/graph-simple.js",
      "require": "./dist/graph-simple.js"
    },
    "./state": {
      "types": "./dist/state.d.ts",
      "import": "./dist/state.js",
      "require": "./dist/state.js"
    },
    "./nodes": {
      "types": "./dist/nodes/index.d.ts",
      "import": "./dist/nodes/index.js",
      "require": "./dist/nodes/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/adapter.d.ts",
      "import": "./dist/mcp/adapter.js",
      "require": "./dist/mcp/adapter.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "echo 'backup: build skipped'",
    "dev": "echo 'backup: dev skipped'",
    "test": "echo 'backup: tests skipped'",
    "test:watch": "echo 'backup: watch skipped'",
    "test:coverage": "echo 'backup: coverage skipped'",
    "test:ci": "echo 'backup: ci tests skipped'",
    "gates:determinism": "echo 'backup: gates skipped'",
    "lint": "echo 'backup: lint skipped'",
    "clean": "rm -rf dist",
    "test:safe": "echo 'backup: tests skipped'"
  },
  "dependencies": {
    "@langchain/core": "^0.3.30",
    "@langchain/langgraph": "^0.4.9",
    "nanoid": "^5.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cortex-os",
    "kernel",
    "langgraph",
    "deterministic",
    "mcp",
    "prp"
  ],
  "author": "Cortex-OS Team",
  "license": "(Apache-2.0 OR Commercial)"
}

{
	"name": "@cortex-os/simlab",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./runner": "./dist/runner.js",
		"./agent-adapter": "./dist/agent-adapter.js",
		"./judge": "./dist/judge.js",
		"./report": "./dist/report.js",
		"./types": "./dist/types.js",
		"./user-sim": "./dist/user-sim.js"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"dev": "tsx watch src/index.ts",
		"simlab:smoke": "tsx src/scripts/run-smoke.ts",
		"simlab:critical": "tsx src/scripts/run-critical.ts",
		"simlab:full": "tsx src/scripts/run-full.ts",
		"simlab:report": "tsx src/scripts/generate-report.ts",
		"simlab:gates": "tsx src/scripts/check-gates.ts",
		"sim:status": "node -e \"console.log('SimLab status coming soon')\""
	}
}

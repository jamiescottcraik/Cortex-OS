{
  "name": "@cortex-os/memories",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": { ".": { "types": "./dist/index.d.ts", "import": "./dist/index.js" } },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --dts --format esm --clean",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prisma:gen": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate deploy --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "eslint": "^9.11.1",
    "prisma": "^5.18.0",
    "tsup": "^8.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "peerDependencies": { "@prisma/client": "^5.18.0" }
}

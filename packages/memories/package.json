{
  "name": "@cortex-os/memories",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "node ../../scripts/vitest-safe.mjs run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prisma:gen": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "test:coverage": "vitest --coverage",
    "jsonl:import": "tsx src/tools/jsonl-to-memory.ts",
    "test:safe": "node ../../scripts/vitest-safe.mjs run"
  },
  "dependencies": {
    "@cortex-os/a2a-contracts": "workspace:*",
    "@cortex-os/a2a-core": "workspace:*",
    "@cortex-os/a2a-transport": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "zod": "^3.23.8",
    "better-sqlite3": "^12.2.0",
    "sqlite-vec": "^0.1.7-alpha.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^9.33.0",
    "prisma": "^5.18.0",
    "@types/better-sqlite3": "^7.6.9",
    "tsup": "^8.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.9.2",
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "@prisma/client": "^5.18.0"
  }
}
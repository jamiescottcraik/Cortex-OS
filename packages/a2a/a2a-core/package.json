    "@cortex-os/a2a-contracts": "workspace:*",
    "@cortex-os/a2a-events": "workspace:*",
    "@cortex-os/contracts": "workspace:*",
    "@cortex-os/telemetry": "workspace:*",
    "axios": "^1.12.2",
    "zod": "^3.25.76"

{
  "name": "@cortex-os/mcp-registry",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./fs-store": {
      "import": "./dist/fs-store.js",
      "types": "./dist/fs-store.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "@cortex-os/mcp-core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}

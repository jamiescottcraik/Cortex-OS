{
  "name": "@cortex-os/mcp-transport",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./stdio": { "import": "./dist/stdio.js", "types": "./dist/stdio.d.ts" },
    "./sse": { "import": "./dist/sse.js", "types": "./dist/sse.d.ts" },
    "./https": { "import": "./dist/https.js", "types": "./dist/https.d.ts" },
    ".": { "import": "./dist/index.js", "types": "./dist/index.d.ts" }
  },
  "scripts": { "build": "tsup src/index.ts src/stdio.ts src/sse.ts src/https.ts --dts --format esm", "test": "vitest run", "typecheck": "tsc -p tsconfig.json --noEmit" },
  "devDependencies": { "tsup": "^8.0.0", "typescript": "^5.5.4" }
}

# Example environment variables for cortex-ai-github
# Copy this to .env and fill in your values

# GitHub token with access to Models API (required)
GITHUB_TOKEN=ghp_your_token_here

# Webhook secret (must match the secret in your GitHub App settings)
WEBHOOK_SECRET=your_webhook_secret_here

# Server port (default: 3001)
PORT=3001

# Optional: GitHub Models API configuration
GITHUB_MODELS_BASE_URL=https://models.inference.ai.azure.com
GITHUB_DEFAULT_MODEL=claude-3-5-sonnet
GITHUB_MAX_TOKENS=4096
GITHUB_TEMPERATURE=0.3

{
	"name": "simple-tests",
	"$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "simple-tests",
	"targets": {
		"test": {
			"executor": "@nx/vite:test",
			"outputs": [
				"{workspaceRoot}/coverage/{projectRoot}"
			],
			"options": {
				"configFile": "simple-tests/vitest.config.ts",
				"reportsDirectory": "coverage/simple-tests",
				"passWithNoTests": false
			},
			"dependsOn": [],
			"configurations": {
				"ci": {
					"coverage": true
				}
			}
		}
	},
	"tags": [
		"scope:testing",
		"type:tests"
	]
}

{
  "name": "@brain/rag-deprecated",
  "version": "1.0.0",
  "description": "RAG (Retrieval-Augmented Generation) system components for Cortex OS",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./chunkers": {
      "import": "./dist/chunkers/index.js",
      "types": "./dist/chunkers/index.d.ts"
    },
    "./pipeline": {
      "import": "./dist/pipeline/index.js",
      "types": "./dist/pipeline/index.d.ts"
    },
    "./policy": {
      "import": "./dist/policy/index.js",
      "types": "./dist/policy/index.d.ts"
    },
    "./prefilter": {
      "import": "./dist/prefilter/index.js",
      "types": "./dist/prefilter/index.d.ts"
    },
    "./index": {
      "import": "./dist/index/index.js",
      "types": "./dist/index/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*.proto"
  ],
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:integration": "vitest run --reporter=verbose integration.test.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "@cortex-os/security": "workspace:*",
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "faiss",
    "vector-search",
    "chunking",
    "document-processing",
    "embeddings",
    "similarity-search",
    "content-addressed",
    "deterministic"
  ],
  "author": "Cortex OS Team",
  "license": "(Apache-2.0 OR Commercial)",
  "repository": {
    "type": "git",
    "url": "https://github.com/cortex-os/cortex-os",
    "directory": "packages/rag"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

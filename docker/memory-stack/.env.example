# Qdrant Configuration
QDRANT_API_KEY=  # Optional: Set if using authentication

# CORS Configuration
CORS_ORIGIN=*  # Set to specific domain in production

# Swagger/OpenAPI
ENABLE_SWAGGER=true  # Set to false in production

# Performance Tuning
MEMORY_DEFAULT_LIMIT=10
MEMORY_MAX_LIMIT=100
MEMORY_DEFAULT_THRESHOLD=0.5
MEMORY_HYBRID_WEIGHT=0.6

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX=1000  # requests per window

# Logging
MEMORY_LOG_LEVEL=info
API_LOG_LEVEL=info
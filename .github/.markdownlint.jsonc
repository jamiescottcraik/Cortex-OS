{
  // Agent instructions and large READMEs often include HTML/image badges and longer headings.
  "MD013": {
    "line_length": 160,
    "code_block_line_length": 9999,
    "heading_line_length": 120,
    "tables": false
  },
  "MD033": {
    "allowed_elements": [
      "sup",
      "sub",
      "img",
      "br",
      "details",
      "summary",
      "a"
    ]
  },
  "MD041": {
    "allow_preamble": true
  }
}
